{
  "name": "serverless_testing_library",
  "description": "serverless_testing_library is a small library to make integration or smoke testing your API's easier.",
  "version": "0.0.0-alpha",
  "keywords": [
    "serverless",
    "API",
    "testing",
    "aws",
    "mocha",
    "jest"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/janders223/serverless_testing_library.git"
  },
  "homepage": "https://github.com/janders223/serverless_testing_library",
  "bugs": {
    "url": "https://github.com/janders223/serverless_testing_library/issues"
  },
  "main": "src/index.js",
  "scripts": {
    "commitmsg": "commitlint -E GIT_PARAMS",
    "lint": "eslint src/**/*.js",
    "semantic-release": "semantic-release",
    "test": "NODE_ENV=test jest",
    "test:ci": "NODE_ENV=test jest --ci --coverage"
  },
  "author": {
    "name": "Jim Anders",
    "email": "jim@janders223.com"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "express": "4.16.4",
    "jest-diff": "23.6.0",
    "jest-matcher-utils": "23.6.0",
    "js-yaml": "3.12.0",
    "supertest": "3.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "babel-eslint": "8.2.6",
    "eslint": "4.19.1",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-prettier": "3.0.0",
    "husky": "1.1.2",
    "jest": "23.6.0",
    "lint-staged": "7.3.0",
    "prettier": "1.14.3",
    "semantic-release": "^15.9.8"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
